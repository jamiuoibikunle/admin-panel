{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "components/home/Navbar.jsx", "components/home/Header.jsx", "components/home/Services.jsx", "pages/_app.jsx", "pages/index.ts", "pages/blog/[slug].jsx", "requests/ind.ts", "pages/[slug].js", "pages/[slug].js", "components/write/Main.tsx", "components/write/backup.js"],
  "exclude": ["node_modules"]
}
